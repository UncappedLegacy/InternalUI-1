	"gold",
	"gold",
	"gold",
	"red",
	"gold",
	"blue",
	"blue",
	"blue",
	"red",
	"red",
	"red",
	"red",
	"gold",
	"red",
	"red",
	
	}
	phr = {
	"and",
	"break",
	"do",
	"else",
	"elseif",
	"end",
	"false",
	"for",
	"function",
	"game",
	"if",
	" in ",
	"local",
	"nil",
	"not",
	"next",
	"pairs",
	"repeat",
	"return",
	"then",
	"true",
	"until",
	"while",
	"1",
	"2",
	"3",
	"4",
	"5",
	"6",
	"7",
	"8",
	"9",
	"0",
	"#",
	"Players",
	"Get",
	"script",
	"Parent",
	"Connect",
	"Wait",
	"wait",
	"Children",
	"_VERSION",
	"_namecall",
	"print",
	}
	local blue,red,gold,green,white,cc;
	
	blue = "93,135,255";
	red = "240,71,71";
	gold = "255,218,71";
	green = "67,181,129";
	print("|---------------------------------------|")
	warn("Syntax Highlighting made by Agent#9336")
	print("|---------------------------------------|")
	local f = "<font color='rgb("
	local fe = ")'>";
	local ft = "</font>";
	warn("*Loaded all vars.")
	---------------------------------| Main Highlighting
	
	
	function h(s)
	s = tostring(s)
	local source,dest,output = phr,Syn,""
	for pos = 1,#s do
	
	for k,v in pairs(source) do
	s = s:gsub(source[k],dest[k]..source[k]..ft)
	
	end
	if s:find("blue") then
	
	s = s:gsub("blue", f..blue..fe)
	end
	if s:find("gold") then
	
	s = s:gsub("gold", f..gold..fe)
	end
	if s:find("red") then
	
	s = s:gsub("red", f..red..fe)
	end
	if s:find("green") then
	s = s:gsub("green", f..green..fe)
	end
	
	return(s)
	end
	
	
	return(s)
	end
	
	while wait() do
	script.Parent.Text = h(script.Parent.Parent.TextBox.Text)
	end
	
	
	
end
coroutine.wrap(ALKRM_fake_script)()
local function VQQHMY_fake_script() -- ExecutorMinimize.LocalScript 
	local script = Instance.new('LocalScript', ExecutorMinimize)

	_G.ExecutorFrameShown = true
	script.Parent.MouseButton1Click:Connect(function()
		
		if _G.ExecutorFrameShown == true then
		script.Parent.Parent.Contents.Visible = false
		script.Parent.Parent.Size = UDim2.new(0, 460,0, 36)
		script.Parent.Position = UDim2.new(0.934, 0,0.214, 0)
		script.Parent.Parent.Seperator.Position = UDim2.new(0, 0,0.97, 0)
		script.Parent.Image = "rbxassetid://7072720870"
		else
		script.Parent.Parent.Contents.Visible = true
		script.Parent.Parent.Size = UDim2.new(0, 460,0, 292)
		script.Parent.Position = UDim2.new(0.934, 0,0.025, 0)
		script.Parent.Parent.Seperator.Position = UDim2.new(0, 0,0.121, 0)
		script.Parent.Image = "rbxassetid://7072719338"
		end
		_G.ExecutorFrameShown = not _G.ExecutorFrameShown
	end)
	--{0, 165},{0, 485}
end
coroutine.wrap(VQQHMY_fake_script)()
