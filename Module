local function RSYL_fake_script() -- ApplyJp.LocalScript 
	local script = Instance.new('LocalScript', ApplyJp)

	wait(4)
	print(game.Workspace.Gravity)
	print(game.Players.LocalPlayer.Character.Humanoid.JumpPower)
	print(game.Players.LocalPlayer.Character.Humanoid.WalkSpeed)
	script.Parent.MouseButton1Click:Connect(function()
		game.StarterPlayer.CharacterUseJumpPower = true
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = script.Parent.Parent.JpInput.Text
	end)
end
