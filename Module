local function VPEX_fake_script() -- PlayerMinimize.LocalScript 
	local script = Instance.new('LocalScript', PlayerMinimize)

	_G.PlayerFrameShown = true
	script.Parent.MouseButton1Click:Connect(function()
		
		if _G.PlayerFrameShown == true then
		script.Parent.Parent.Contents.Visible = false
		script.Parent.Parent.Size = UDim2.new(0, 165,0, 34)
		script.Parent.Position = UDim2.new(0.83, 0,0.191, 0)
		script.Parent.Parent.Seperator.Position = UDim2.new(0, 0,0.96, 0)
		script.Parent.Image = "rbxassetid://7072720870"
		else
		script.Parent.Parent.Contents.Visible = true
		script.Parent.Parent.Size = UDim2.new(0, 165,0, 485)
		script.Parent.Position = UDim2.new(0.83, 0,0.012, 0)
		script.Parent.Parent.Seperator.Position = UDim2.new(0, 0,0.066, 0)
		script.Parent.Image = "rbxassetid://7072719338"
		end
		_G.PlayerFrameShown = not _G.PlayerFrameShown
	end)
	--{0, 165},{0, 485}
end
