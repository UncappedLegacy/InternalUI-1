-- Scripts:

local function VKXH_fake_script() -- Module.LocalScript 
	local script = Instance.new('LocalScript', Module)

	local FrameObject = script.Parent -- Change Stats to whatever your frame is called
	local Open = false
	
	
	local UserInputService = game:GetService("UserInputService")
	
	UserInputService.InputBegan:Connect(function(Input, gameprocess)
		if not gameprocess then
			if Input.KeyCode == Enum.KeyCode.RightShift then -- Change M to your prefered keybind
				script.Parent.Enabled = not script.Parent.Enabled
			end
		end
	
	end)
end
coroutine.wrap(VKXH_fake_script)()
local function EOVMT_fake_script() -- Contents.LocalScript 
	local script = Instance.new('LocalScript', Contents)

	Colors = {
	BLUE = "0,120,255",
	GREEN = "0,120,255", 
	BLACK = "0,0,0",
	AQUA = "0,120,255"
	}
	
	Syntax = {
		BLUE = {"and", "break", "do", "else", "elseif", "end", "false", "for", "function", "if", "in", "local", "nil", "not", "or", "repeat", "return", "then", "true", "until", "while"}
	}
	
	local fc = '<font color="rgb('
	local fce = ')">'
	
	
end
coroutine.wrap(EOVMT_fake_script)()
